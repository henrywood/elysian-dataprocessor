{
    "name": "elysian/dataprocessor",
    "description": "High-performance data import/export library with cloud storage support, chunking, and Swoole coroutines",
    "type": "library",
    "keywords": [
        "data-processing",
        "import",
        "export",
        "excel",
        "csv",
        "openspout",
        "swoole",
        "coroutines",
        "cloud-storage",
        "chunking",
        "performance"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Elysian Development Team",
            "email": "dev@elysian.com",
            "homepage": "https://elysian.com"
        }
    ],
    "homepage": "https://github.com/elysian/dataprocessor",
    "support": {
        "issues": "https://github.com/elysian/dataprocessor/issues",
        "source": "https://github.com/elysian/dataprocessor",
        "docs": "https://github.com/elysian/dataprocessor/blob/main/README.md"
    },
    "require": {
        "php": "^8.1",
        "openspout/openspout": "^4.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "suggest": {
        "ext-swoole": "For high-performance queue processing with coroutines",
        "aws/aws-sdk-php": "For Amazon S3 cloud storage support",
        "google/cloud-storage": "For Google Cloud Storage support",
        "microsoft/azure-storage-blob": "For Azure Blob Storage support",
        "ext-pdo": "For database operations in import/export classes"
    },
    "autoload": {
        "psr-4": {
            "Elysian\\DataProcessor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elysian\\DataProcessor\\Tests\\": "tests/",
            "Elysian\\DataProcessor\\Examples\\": "examples/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "analyse": "phpstan analyse src --level=8",
        "cs-fix": "php-cs-fixer fix",
        "cs-check": "php-cs-fixer fix --dry-run --diff",
        "check": [
            "@cs-check",
            "@analyse",
            "@test"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
